{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.0.0, 5.0.0)"
  },
  "extensions": {
    "http": {
      "routePrefix": ""
    }
  },
  "logging": {
    "fileLoggingMode": "debugOnly",
    "logLevel": {
      "default": "None",
      "Host.Results": "None",
      "Function": "Trace",
      "Host.Aggregator": "None"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true
    }
  } 
}
}
